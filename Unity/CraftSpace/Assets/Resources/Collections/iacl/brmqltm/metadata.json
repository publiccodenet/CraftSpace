ate": "2003-11-18 15:50:08", "publicdate": "2005-10-05 15:31:10", "filesxml": "Thu Dec 14 21:55:14 UTC 2006", "backup_location": "ia903600_10", "openlibrary_work": "OL2347633W", "openlibrary_edition": "OL15490830M"}, "reviews": [{"reviewbody": "A fantastic book. Colors are beautiful. The story is far superior to just words. The book is a true blessing.", "reviewtitle": "The Quiltmakers Gift", "reviewer": "ladylace", "reviewdate": "2009-10-07 13:23:40", "createdate": "2009-10-07 13:23:40", "stars": "5"}], "server": "ia902208.us.archive.org", "uniq": 968688598, "workable_servers": ["ia902208.us.archive.org", "ia802208.us.archive.org"]}
